<html xmlns:jsp="http://java.sun.com/JSP/Page"
      xmlns:c="http://java.sun.com/jsp/jstl/core"
      xmlns:tiles="http://tiles.apache.org/tags-tiles"
      xmlns:spring="http://www.springframework.org/tags"
      xmlns:util="urn:jsptagdir:/WEB-INF/tags/util">

<jsp:output doctype-root-element="HTML"
            doctype-system="about:legacy-compat"/>

<jsp:directive.page contentType="text/html;charset=UTF-8"/>
<jsp:directive.page pageEncoding="UTF-8"/>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=8"/>

    <util:load-scripts/>

    <script src="/script/javascript/enhancedBrowser/base.js"
            type="text/javascript"><!-- /required for FF3 and Opera --></script>
    <script src="/script/javascript/enhancedBrowser/autoRun.js"
            type="text/javascript"><!-- /required for FF3 and Opera --></script>


    <spring:message code="application_name" var="app_name"
                    htmlEscape="false"/>
    <title><spring:message code="welcome_h3" arguments="${app_name}"/></title>
</head>

<body class="tundra spring">
<div id="wrapper">

    <tiles:insertAttribute name="body"/>

    <div id="enhancedBrowser_topBar">


        AutoRun:

        <ul >

            <li class="listTopBar">
                <a href="#" onclick="startAutoRun()">Start</a> | <a href="#" onclick="stopAutoRun()">Stop</a>
            </li>
            <li class="listTopBar">
               ........... <a href="#" onclick="changeUrlStatus('PROCESSED')"> (P)</a> | <a href="#"
                                                                                 onclick="changeUrlStatus('BLOCKED')"> (B) </a>
            </li>
        </ul>


    </div>


    <div class="enhancedBrowser_sidebarActivator">
        _

    </div>

    <div id="enhancedBrowser_sidebar" class="enhancedBrowser_sidebar">
        <strong>Enhanced Browser Menu:</strong>
        <br/>
        <br/>
        Update current site:
        <br/>
        <br/>
        <a href="#" onclick="changeUrlStatus('PROCESSED')"> (P)</a> | <a href="#"
                                                                         onclick="changeUrlStatus('BLOCKED')"> (B) </a>
        <br/>
        <br/>
        <a href="#" onclick="setCurrentStatus('ACTIVE','urlListHolder')">- Show New</a>
        <br/>
        <a href="#"
           onclick="setCurrentStatus('PROCESSED','urlListHolder')">- Show Processed</a>
        <br/>
        <a href="#" onclick="setCurrentStatus('BLOCKED','urlListHolder')">- Show Blocked</a>
        <br/>

        <br/>

        <div id="urlListHolder">
            _
        </div>
        <br/>
        <br/>
        <br/>


        <div id="campaignSelectAjax">
            _
        </div>

        <div id="websiteSelectAjax">
            _
        </div>

    </div>

    <div id="enhancedBrowser_iframe">

        <iframe src='http://indeed.com' id='iframeMain' width="100%" height="100%">

        </iframe>
        <br/>
        <br/>
        <br/>
    <span id='indeed_at'><a href="http://www.indeed.com/">jobs</a> by <a
            href="http://www.indeed.com/" title="Job Search"><img
            src="http://www.indeed.com/p/jobsearch.gif" style="border: 0;
vertical-align: middle;" alt="Indeed job search"/></a></span>

    </div>


</div>
</body>
</html>
